<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>ClientServlet</servlet-name>
    <servlet-class>com.typ.web.controller.ClientServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ControlServlet</servlet-name>
    <servlet-class>com.typ.web.controller.ControlServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PassportServlet</servlet-name>
    <servlet-class>com.typ.web.controller.PassportServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>ClientServlet</servlet-name>
    <url-pattern>/servlet/ClientServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ControlServlet</servlet-name>
    <url-pattern>/servlet/ControlServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PassportServlet</servlet-name>
    <url-pattern>/servlet/PassportServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>